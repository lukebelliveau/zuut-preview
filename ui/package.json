{
  "name": "zuut",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@apollo/client": "^3.5.9",
    "@auth0/auth0-react": "^1.8.0",
    "@babel/preset-typescript": "^7.16.7",
    "@getgauge/cli": "latest",
    "@reduxjs/toolkit": "^1.7.1",
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^24.9.1",
    "@types/jest-axe": "^3.5.3",
    "@types/node": "^12.20.38",
    "@types/react": "^16.14.21",
    "@types/react-dom": "^16.9.14",
    "@types/react-helmet": "6.1.5",
    "@types/react-redux": "^7.1.21",
    "@types/redux-logger": "^3.0.9",
    "@types/testing-library__cypress": "^5.0.9",
    "@types/uuid": "^8.3.3",
    "@typescript-eslint/eslint-plugin": "^5.8.0",
    "@typescript-eslint/parser": "^5.8.0",
    "clsx": "^1.1.1",
    "eslint": "^8.5.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jest": "^25.3.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "graphql": "^16.3.0",
    "jest-axe": "^5.0.1",
    "jest-junit": "^13.0.0",
    "konva": "^8.3.2",
    "prettier": "^2.5.1",
    "react": "^17.0.2",
    "react-dnd": "^14.0.5",
    "react-dnd-html5-backend": "^14.1.0",
    "react-dom": "^17.0.2",
    "react-full-page": "^0.1.12",
    "react-helmet-async": "^1.2.2",
    "react-hook-form": "^7.22.5",
    "react-konva": "^17.0.2-5",
    "react-redux": "^7.2.6",
    "react-router-dom": "^6.2.1",
    "react-scripts": "5.0.0",
    "redux-logger": "^3.0.6",
    "taiko": "latest",
    "typescript": "~4.1.5",
    "uuid": "^8.3.2",
    "web-vitals": "^2.1.2"
  },
  "scripts": {
    "build": "react-scripts build",
    "eject": "react-scripts eject",
    "format": "eslint --fix src",
    "lint": "tsc --noEmit && eslint src",
    "prepush": "npm run lint -- --max-warnings=0 && CI=true npm test && npm run taiko-test",
    "start": "react-scripts start",
    "test": "react-scripts test",
    "taiko-test": "gauge run specs",
    "dev": "npm start",
    "cypress:open": "cypress open",
    "cypress:local": "CYPRESS_baseUrl=http://localhost:3000 cypress open"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest",
      "plugin:jsx-a11y/recommended",
      "prettier"
    ],
    "rules": {
      "@typescript-eslint/no-unused-vars": [
        "error",
        {
          "varsIgnorePattern": ".*"
        }
      ],
      "quotes": [
        "error",
        "single",
        "avoid-escape"
      ],
      "semi": [
        "error",
        "always"
      ],
      "testing-library/prefer-screen-queries": "off"
    },
    "ignorePatterns": [
      "**/*.js"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/cypress": "^8.0.2",
    "@testing-library/react-hooks": "^7.0.2",
    "cypress": "^9.5.0",
    "eslint-config-prettier": "^8.3.0"
  },
  "jest": {
    "transformIgnorePatterns": [
      "<rootDir>/node_modules/(?!konva)"
    ]
  }
}
